{
  "multipart": [

    {   "when": { "OR": [
        { "facing": "north|south"}
        ]},
      "apply": { "model": "mcwbridgesabnormals:block/bridge/rail/river_base", "y": 90}
    },
	{   "when": { "OR": [
        { "facing": "east|west"}
        ]},
      "apply": { "model": "mcwbridgesabnormals:block/bridge/rail/river_base", "y": 0}
    },

	{
      "when": { "AND": [  {"north": "true","east": "true","south": "true","west": "true"} ]},
      "apply": { "model": "mcwbridgesabnormals:block/bridge/rail/river_core", "y": 0}
    },
	

    {
      "when": { "AND": [  {"north": "true","west": "true","east": "false","south": "false", "north_west": "true"} ]},
      "apply": { "model": "mcwbridgesabnormals:block/bridge/rail/river_corner", "y": 180}
    },
	{
      "when": { "AND": [  {"north": "true","east": "true","west": "false","south": "false", "north_east": "true"} ]},
      "apply": { "model": "mcwbridgesabnormals:block/bridge/rail/river_corner", "y": 270}
    },
	{
      "when": { "AND": [  {"south": "true","east": "true","west": "false","north": "false", "south_east": "true"} ]},
      "apply": { "model": "mcwbridgesabnormals:block/bridge/rail/river_corner"}
    },
	{
      "when": { "AND": [  {"south": "true","west": "true","east": "false","north": "false", "south_west": "true"} ]},
      "apply": { "model": "mcwbridgesabnormals:block/bridge/rail/river_corner", "y": 90}
    },
	
    {
      "when": { "AND": [  {"north": "true","west": "true","east": "false","south": "false", "north_west": "false"} ]},
      "apply": { "model": "mcwbridgesabnormals:block/bridge/rail/river_corner", "y": 180}
    },
	{
      "when": { "AND": [  {"north": "true","east": "true","west": "false","south": "false", "north_east": "false"} ]},
      "apply": { "model": "mcwbridgesabnormals:block/bridge/rail/river_corner", "y": 270}
    },
	{
      "when": { "AND": [  {"south": "true","east": "true","west": "false","north": "false", "south_east": "false"} ]},
      "apply": { "model": "mcwbridgesabnormals:block/bridge/rail/river_corner"}
    },
	{
      "when": { "AND": [  {"south": "true","west": "true","east": "false","north": "false", "south_west": "false"} ]},
      "apply": { "model": "mcwbridgesabnormals:block/bridge/rail/river_corner", "y": 90}
    },
		
	{
      "when": { "AND": [  {"north": "true", "east": "true","south": "false", "west": "true"} ]},
      "apply": { "model": "mcwbridgesabnormals:block/bridge/rail/river_side"} 
    },		
	{
      "when": { "AND": [  {"north": "false", "east": "true","south": "true", "west": "true"} ]},
      "apply": { "model": "mcwbridgesabnormals:block/bridge/rail/river_side", "y": 180} 
    },	
	{
      "when": { "AND": [  {"north": "true", "east": "true","south": "true", "west": "false"} ]},
      "apply": { "model": "mcwbridgesabnormals:block/bridge/rail/river_side", "y": 90} 
    },	
	{
      "when": { "AND": [  {"north": "true", "east": "false","south": "true", "west": "true"} ]},
      "apply": { "model": "mcwbridgesabnormals:block/bridge/rail/river_side", "y": 270} 
    },	
		
    {
      "when": { "AND": [  {"north": "false", "south": "true", "west": "false", "east": "false"} ]},
      "apply": { "model": "mcwbridgesabnormals:block/bridge/rail/river_middle", "y": 180}
    },
    {
      "when": { "AND": [  {"south": "false","north": "true", "west": "false", "east": "false"} ]},
      "apply": { "model": "mcwbridgesabnormals:block/bridge/rail/river_middle"}
    },
    {
      "when": { "AND": [  {"east": "false","west": "true", "north": "false", "south": "false"} ]},
      "apply": { "model": "mcwbridgesabnormals:block/bridge/rail/river_middle", "y": 270}
    },
  	{
      "when": { "AND": [  {"west": "false","east": "true", "north": "false", "south": "false"} ]},
      "apply": { "model": "mcwbridgesabnormals:block/bridge/rail/river_middle", "y": 90}
    },
	{
      "when": { "AND": [  {"east": "true", "west": "true", "north": "false", "south": "false"} ]},
      "apply": { "model": "mcwbridgesabnormals:block/bridge/rail/river_middle", "y": 90}
    },
	{
      "when": { "AND": [  {"south": "true", "north": "true", "east": "false", "west": "false"} ]},
      "apply": { "model": "mcwbridgesabnormals:block/bridge/rail/river_middle"}
    },
	{
      "when": { "AND": [  {"facing": "north|south","south": "false", "east": "false", "north": "false", "west": "false"} ]},
      "apply": { "model": "mcwbridgesabnormals:block/bridge/rail/river_middle", "y": 90}
    },
	{
      "when": { "AND": [  {"facing": "east|west","south": "false", "east": "false", "north": "false", "west": "false"} ]},
      "apply": { "model": "mcwbridgesabnormals:block/bridge/rail/river_middle"}
    }
  ]
}	
